<h2 class="head"> List of Product </h2>
<br class="clear">
  <div class="product" >
    <% @products.each do|f|%>
      <ul >
        <li>
          <p>Name	:<%= f.name %></p>
          <p>Price	:<%= f.price %></p>
        <% if !current_user.nil? %>
        <p><%= link_to "See More", product_path(f.id) %><%= link_to "Edit", edit_product_path(f.id) %></p>
        <% end %>
        </li>
    <% end %>
    </ul>
  </div>


